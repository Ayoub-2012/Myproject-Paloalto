import requests
import json

url = "https://192.168.181.134/restapi/v10.1/Objects/Addresses?location=vsys&vsys=vsys1&name=Test"

payload = json.dumps({
  "entry": {
    "@name": "Test",
    "ip-netmask": "10.1.1.1/32"
  }
})
headers = {
  'Content-Type': 'application/json',
  'X-PAN-KEY': '••••••',
  'Cookie': 'PHPSESSID=5ccff29814c01f2527fe3ec91fb70edc'
}

response = requests.request("POST", url, headers=headers, data=payload)

print(response.text)
